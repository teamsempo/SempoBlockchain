{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "REPOSITORY_URI:server_TAG_SUFFIX",
      "essential": true,
      "memoryReservation": 256,
      "cpu": 1,
      "links": ["pgbouncer:pgbouncer"],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "APP"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "pgbouncer",
      "image": "REPOSITORY_URI:pgbouncer_TAG_SUFFIX",
      "essential": true,
      "memory": 32,
      "cpu": 1,
      "environment": [
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        }
      ],
      "portMappings": [
        {
          "hostPort": 6543,
          "containerPort": 6543
        }
      ],
      "links": []
    },
    {
      "name": "proxy",
      "image": "REPOSITORY_URI:proxy_TAG_SUFFIX",
      "essential": true,
      "memory": 32,
      "cpu": 1,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": ["app:app"]
    },
    {
      "name": "high_pri_eth_worker",
      "image": "REPOSITORY_URI:eth_worker_TAG_SUFFIX",
      "essential": false,
      "memory": 256,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "HIGH_PRIORITY_WORKER"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "low_pri_eth_worker",
      "image": "REPOSITORY_URI:eth_worker_TAG_SUFFIX",
      "essential": true,
      "memory": 128,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "LOW_PRIORITY_WORKER"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "eth_processor",
      "image": "REPOSITORY_URI:eth_worker_TAG_SUFFIX",
      "essential": true,
      "memory": 128,
      "links": ["pgbouncer:pgbouncer"],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "PROCESSOR"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    }
  ]
}
