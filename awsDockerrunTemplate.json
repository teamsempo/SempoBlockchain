{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "REPOSITORY_URI:server",
      "essential": true,
      "memoryReservation": 256,
      "cpu": 1,
      "links": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "APP"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "proxy",
      "image": "REPOSITORY_URI:proxy",
      "essential": true,
      "memory": 64,
      "cpu": 1,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "links": ["app:app"]
    },
    {
      "name": "eth_worker",
      "image": "REPOSITORY_URI:eth_worker",
      "essential": false,
      "memory": 256,
      "links": [],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "PRIMARY"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    },
    {
      "name": "eth_processor",
      "image": "REPOSITORY_URI:eth_worker",
      "essential": false,
      "memory": 256,
      "links": [],
      "mountPoints": [],
      "environment": [
        {
          "name": "CONTAINER_TYPE",
          "value": "PROCESSOR"
        },
        {
          "name": "SERVER_HAS_S3_AUTH",
          "value": true
        },
        {
          "name": "PYTHONUNBUFFERED",
          "value": 0
        }
      ]
    }
  ]
}
